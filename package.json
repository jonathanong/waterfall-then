{
  "name": "waterfall-then",
  "description": "waterfall/sequential chaining of promise-returning functions",
  "version": "2.0.1",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jonathanong/waterfall-then",
  "dependencies": {
    "lodash": "^3.10.1",
    "native-or-bluebird": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "4",
    "bluebird": "^3.1.1",
    "codecov": "1",
    "eslint": "1",
    "eslint-config-jongleberry": "1",
    "eslint-plugin-react": "^3.13.1",
    "eslint-plugin-standard": "1",
    "ghooks": "1",
    "mocha": "2",
    "nyc": "5"
  },
  "scripts": {
    "lint": "eslint --cache lib test",
    "test": "mocha",
    "test-cov": "nyc node_modules/mocha/bin/_mocha && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
