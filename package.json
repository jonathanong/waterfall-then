  "version": "2.1.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jonathanong/waterfall-then",
  "dependencies": {
    "any-promise": "^1.1.0",
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "bluebird": "^3.1.1",
    "codecov": "^1.0.0",
    "mocha": "^2.0.0",
    "nyc": "^6.0.0",
